{
  "name": "svelte-app",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
  "build": "rollup -c",
  "dev": "concurrently \"rollup -c -w\" \"sirv public --no-clear --dev --single\"",
  "start": "sirv public --no-clear --single",
  "push-and-dev": "git add . && git commit -m 'Auto commit before dev' && git push origin main && concurrently \"rollup -c -w\" \"sirv public --no-clear --dev --single\""
},
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.1.0",
    "@rollup/plugin-node-resolve": "^15.3.1",
    "@rollup/plugin-terser": "^0.4.4",
    "concurrently": "^9.1.2",
    "rollup": "^3.15.0",
    "rollup-plugin-css-only": "^4.5.2",
    "rollup-plugin-livereload": "^2.0.5",
    "rollup-plugin-svelte": "^7.1.2",
    "svelte": "^3.55.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.7.2",
    "@fortawesome/free-solid-svg-icons": "^6.7.2",
    "@fortawesome/svelte-fontawesome": "^0.2.3",
    "@stripe/stripe-js": "^5.7.0",
    "@supabase/supabase-js": "^2.49.1",
    "sirv-cli": "^2.0.0",
    "svelte-fa": "^4.0.3",
    "svelte-routing": "^2.13.0",
    "uuid": "^11.1.0"
  }
}
